.head {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 20rpx;
  font-size: 100rpx;
}

.image-box {
  padding: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1px solid rgba(0, 0, 0, .5);
}

.image,
.image-box {
  border-radius: 40rpx;
}