.container {
  display: flex;
  flex-direction: column;
  gap: 20rpx;
}

.image-box {
  position: relative;
}

.image {
  border-radius: 30rpx;
  height: 300rpx;
  width: 100%;
}

.trash {
  height: 100rpx;
  border-radius: 20rpx;
  position: absolute;
  right: 10rpx;
  top: 10rpx;
  z-index: 100;
  border: black solid 1px;
  background-color: #D9D9D9;
  opacity: 75%;
}