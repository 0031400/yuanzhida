.container {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 30rpx;
  box-sizing: border-box;
}

.image-box {
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin: 30rpx auto;
}

.question-item {
  border-bottom: black 1px solid;
  padding: 10rpx;
}

.title {
  font-size: 60rpx;
}

.content {
  font-size: 40rpx;
}