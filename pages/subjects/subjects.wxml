<scroll-view class="container" scroll-y="true">
  <view class="head">源智答</view>
  <view class="list">
    <view class="image-box" wx:for="{{subjects}}" wx:key="id">
      <navigator url="{{'/pages/questions/questions?categoryId='+item.id}}">
        <image class="image" src="{{item.image}}" mode="widthFix" />
      </navigator>
    </view>
  </view>
</scroll-view>